@model IEnumerable<AspNetCorePostgreSQLDockerApp.Models.JreShow>

@{
    ViewData["Title"] = "JRE Show";
}





<table id="movies" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Link</th>
        </tr>
    </thead>

    <tbody>
            @foreach (var movie in Model)
            {
                <tr>
                    <td>@movie.Title</td>
                    <td><a href=@movie.Url>Link</a></td>
                </tr>
            }
        </tbody>
</table>